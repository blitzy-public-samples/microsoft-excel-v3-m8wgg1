{
  "name": "microsoft-excel",
  "version": "1.0.0",
  "description": "Microsoft Excel implementation for web, desktop, and mobile platforms",
  "main": "src/index.js",
  "scripts": {
    "start:web": "webpack serve --config config/webpack.config.js --env development",
    "start:desktop": "electron .",
    "start:mobile": "react-native start",
    "build": "webpack --config config/webpack.config.js --env production",
    "test": "jest --config config/jest.config.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/excel.git"
  },
  "keywords": [
    "excel",
    "spreadsheet",
    "microsoft",
    "office"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0",
    "@microsoft/office-js": "^1.1.0",
    "electron": "^13.0.0",
    "react-native": "^0.64.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "ts-loader": "^9.1.1",
    "jest": "^26.6.3",
    "eslint": "^7.25.0",
    "prettier": "^2.2.1"
  }
}